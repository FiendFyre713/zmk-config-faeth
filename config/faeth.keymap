#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
// -----------------------------------------------------------------------------------------
// |  Q  |  W  |  F  |  P  |  B  |   |  J  |  L   |  U  |  Y  |  '  |
// |  A  |  R  |  S  |  T  |  G  |   |  M  |  N   |  E  |  I  |  O  |
// |  Z  |  X  |  C  |  D  |  V  |   |  K  |  H   |  ,  |  .  |  /  |
//             | ESC | SPC | TAB |   | ENT | BACK | DEL |
                        bindings = <
   &kp q &kp 7 &kp 8 &kp 9 &kp B   &kp J &kp L  &kp U     &kp Y   &kp APOS
   &kp A &kp 4 &kp 5 &kp 6 &kp G   &kp M &kp N  &kp E     &kp I   &kp O
   &kp Z &kp 1 &kp 2 &kp 3 &kp V   &kp K &kp H  &kp COMMA &kp DOT &kp FSLH
        &kp ESC &kp 0 &kp RET       &kp RET &kp BSPC &kp DEL
                        >;
                };
        };
};
